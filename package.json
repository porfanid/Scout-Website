{
  "name": "scout-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "eslintConfig": {
    "extends": "./eslint.config.js"
  },
  "jest": {
    "preset": "./.config/jest/jest.config.js"
  },
  "scripts": {
    "publish": "node ./scripts/deploy.js",
    "publish-firebase-rules": "firebase deploy --only firestore",
    "dev": "concurrently \"vite --host\" \"php -S localhost:8000 -t public/api\" \"firebase emulators:start\"",
    "build": "vite build",
    "test": "NODE_NO_WARNINGS=1 jest",
    "generate-sitemap": "node scripts/generate-sitemap.cjs",
    "changelog": "conventional-changelog -p react -i .github/CHANGELOG.md -s -r 0",
    "docs": "jsdoc -c jsdoc.json",
    "github-automation": "node scripts/github-automation.cjs",
    "postbuild": "npm run generate-sitemap",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublish": "npm run build",
    "prebuild": "rimraf public/gallery"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/indie-flower": "^5.1.1",
    "@fontsource/montserrat": "^5.1.0",
    "@fontsource/pacifico": "^5.1.0",
    "@fontsource/playfair-display": "^5.1.0",
    "@fontsource/roboto": "^5.1.0",
    "@fontsource/roboto-slab": "^5.1.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.3.0",
    "@mui/styles": "^6.1.6",
    "@react-spring/web": "^9.7.5",
    "add-to-calendar-button-react": "^2.7.3",
    "axios": "^1.7.8",
    "firebase": "^11.0.1",
    "i18next": "^24.0.2",
    "moment": "^2.30.1",
    "next": "^14.2.18",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-big-calendar": "^1.16.3",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.4.0",
    "react-router-dom": "^7.0.1",
    "tr46": "^5.0.0",
    "yet-another-react-lightbox": "^3.21.7"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@eslint/js": "^9.15.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.1.0",
    "conventional-changelog-cli": "^5.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "esm": "^3.2.25",
    "ftp-deploy": "^2.4.7",
    "globals": "^15.12.0",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^12.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc": "^4.0.4",
    "rimraf": "^6.0.1",
    "simple-git": "^3.27.0",
    "sitemap": "^8.0.0",
    "vite": "^5.4.10",
    "vite-plugin-mkcert": "^1.17.6"
  }
}
